{
  "name": "MIA",
  "version": "1.0.0",
  "main": "./ts-built/back-end/index.js",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "4.10.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-htaccess-middleware": "^0.1.0",
    "plume-ts-di": "^1.2.4",
    "prisma": "4.10.1",
    "simple-logging-system": "^1.1.0",
    "tsc": "^2.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@wessberg/di-compiler": "^3.2.0",
    "@yarnpkg/pnpify": "^4.0.0-rc.39",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "prisma-case-format": "file:./lib/prisma-case-format",
    "ts-node": "^10.9.1",
    "ts-transformer-classname": "1.0.4",
    "ttypescript": "^1.5.15",
    "typescript": "4.9.3",
    "vite": "^4.3.2"
  },
  "scripts": {
    "build": "tsc",
    "start": "node ts-built/index.js",
    "dev": "concurrently \"yarn watch-ts\" \"vite --host\" \"yarn watch-ts-node\" \"nodemon -q ts-built/back-end/index.js\"",
    "pull-db": "npx prisma db pull && prisma-case-format --file \"prisma/schema.prisma\" && pnpify prisma generate",
    "watch-ts": "ttsc --project tsconfig.json -w",
    "watch-ts-node": "ttsc --project tsconfig-node.json -w"
  },
  "packageManager": "yarn@3.4.1",
  "dependenciesMeta": {
    "@prisma/client@4.10.1": {
      "unplugged": true
    },
    "prisma@4.10.1": {
      "unplugged": true
    }
  }
}
